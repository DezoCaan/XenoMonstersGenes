		<description>It has ALMOST no rate of fire.</description>
		<iconPath>UI/Icons/Genes/HighLowRPS/XMonsters_LowRPS3</iconPath>
		<displayOrderInCategory>27</displayOrderInCategory>
		<statFactors>
			<RangedCooldownFactor>2</RangedCooldownFactor>
		</statFactors>
		<statOffsets>
			<AimingDelayFactor>1</AimingDelayFactor>
		</statOffsets>
		<suppressedTraits>
			<li>
				<def>ShootingAccuracy</def>
				<degree>1</degree>
			</li>
		</suppressedTraits>
		<biostatCpx>1</biostatCpx>
		<biostatMet>3</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>awfulfiringrate</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
