<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<GeneDef Name="XMonsters_GeneHealingRate" Abstract="True">
		<displayCategory>XMonsters_Health</displayCategory>
		<exclusionTags>
			<li>WoundHealingRate</li>
		</exclusionTags>
	</GeneDef>
	
	<GeneDef ParentName="XMonsters_GeneHealingRate">
		<defName>XMonsters_FastHealing</defName>
		<label>free fast wound healing</label>
		<labelShortAdj>free fast wound healing</labelShortAdj>
		<description>Does not require metabolism for rapid wound healing.</description>
		<iconPath>UI/Icons/Genes/FastHealing/XMonsters_FastWoundHealing1</iconPath>
		<displayOrderInCategory>1</displayOrderInCategory>
		<marketValueFactor>1.2</marketValueFactor>
		<statFactors>
			<InjuryHealingFactor>2</InjuryHealingFactor>
		</statFactors>
		<biostatCpx>2</biostatCpx>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>fastselfheal</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
