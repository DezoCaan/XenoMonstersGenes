<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<GeneDef Name="XMonsters_GeneImmunity" Abstract="True">
		<displayCategory>XMonsters_Health</displayCategory>
		<exclusionTags>
			<li>Immunity</li>
		</exclusionTags>
	</GeneDef>
	
	<GeneDef ParentName="XMonsters_GeneImmunity">
		<defName>XMonsters_SuperWeakestImmunity</defName>
		<label>super weakest immunity</label>
		<labelShortAdj>super weakest immunity</labelShortAdj>
		<description>Just so long as I don't catch any infection or contamination.</description>
		<iconPath>UI/Icons/Genes/GoodBadImmunity/XMonsters_BadImmunity3</iconPath>
		<displayOrderInCategory>6</displayOrderInCategory>
		<statFactors>
			<ImmunityGainSpeed>0.25</ImmunityGainSpeed>
		</statFactors>
		<suppressedTraits>
			<li>
				<def>Immunity</def>
				<degree>-1</degree>
			</li>
		</suppressedTraits>
		<biostatCpx>1</biostatCpx>
		<biostatMet>5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>superweakest</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
