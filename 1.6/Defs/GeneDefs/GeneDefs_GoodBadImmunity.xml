		<iconPath>UI/Icons/Genes/GoodBadImmunity/XMonsters_Immunity1</iconPath>
		<displayOrderInCategory>9</displayOrderInCategory>
		<marketValueFactor>1.25</marketValueFactor>
		<statFactors>
			<ImmunityGainSpeed>1.25</ImmunityGainSpeed>
		</statFactors>
		<biostatCpx>2</biostatCpx>
