<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<GeneDef Name="XMonsters_GeneReloadSpeed" Abstract="True">
		<displayCategory>XMonsters_RangedCombat</displayCategory>
		<exclusionTags>
			<li>XMonsters_ReloadSpeed</li>
		</exclusionTags>
	</GeneDef>
	
	<GeneDef ParentName="XMonsters_GeneReloadSpeed">
		<defName>XMonsters_SlightlyFastReload</defName>
		<label>reload speed - slightly fast</label>
		<labelShortAdj>reload speed - slightly fast</labelShortAdj>
		<description>Carriers of this gene reload any weapon slightly faster.\n\n&lt;color=#e5e500&gt;Note:&lt;/color&gt;\n\nThis gene only works if you have the &lt;color=#b29c2c&gt;Combat&lt;/color&gt; &lt;color=#4e591a&gt;Extended&lt;/color&gt; mod installed.</description>
		<iconPath>UI/Icons/Genes/SpeedReload/XMonsters_SpeedReload1</iconPath>
		<displayOrderInCategory>6</displayOrderInCategory>
		<marketValueFactor>1.2</marketValueFactor>
		<statFactors>
			<ReloadSpeed MayRequire="CETeam.CombatExtended">1.25</ReloadSpeed>
		</statFactors>
		<biostatCpx>2</biostatCpx>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>slightlyfastreload</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
