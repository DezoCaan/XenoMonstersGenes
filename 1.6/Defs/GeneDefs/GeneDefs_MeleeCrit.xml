<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<GeneDef Name="XMonsters_GeneMeleeCrit" Abstract="True">
		<displayCategory>XMonsters_MeleeCombat</displayCategory>
		<exclusionTags>
			<li>XMonsters_MeleeCrit</li>
		</exclusionTags>
	</GeneDef>
	
	<GeneDef ParentName="XMonsters_GeneMeleeCrit">
		<defName>XMonsters_TerriblyLowMeleeCrit</defName>
		<label>terribly low critical hit chance</label>
		<labelShortAdj>terribly low critical hit chance</labelShortAdj>
		<description>Carriers of the gene never or rarely deal critical hits. \n\n&lt;color=#e5e500&gt;Note:&lt;/color&gt; \n\nThis gene only works if you have the &lt;color=#b29c2c&gt;Combat&lt;/color&gt; &lt;color=#4e591a&gt;Extended&lt;/color&gt; mod installed.</description>
		<iconPath>UI/Icons/Genes/MeleeCrit/XMonsters_BadMeleeCrit3</iconPath>
		<displayOrderInCategory>21</displayOrderInCategory>
		<statOffsets>
			<MeleeCritChance MayRequire="CETeam.CombatExtended">-1</MeleeCritChance>
		</statOffsets>
		<biostatCpx>1</biostatCpx>
		<biostatMet>3</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>terriblylowcrit</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
