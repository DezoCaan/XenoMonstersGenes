<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<GeneDef Name="XMonsters_GeneMoveSpeed" Abstract="True">
		<displayCategory>XMonsters_Movement</displayCategory>
		<exclusionTags>
			<li>MoveSpeed</li>
		</exclusionTags>
	</GeneDef>

	<GeneDef ParentName="XMonsters_GeneMoveSpeed">
		<defName>XMonsters_StoneSpeed</defName>
		<label>stone speed</label>
		<labelShortAdj>stone</labelShortAdj>
		<description>In fact, those with this gene possess such incredible movement speed that they don't even realize it. It even suppresses the trait associated with slowing down movement speed.</description>
		<iconPath>UI/Icons/Genes/MoveSpeed/XMonsters_AntiMoveSpeed2</iconPath>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<MoveSpeed>-0.8</MoveSpeed>
		</statOffsets>
		<suppressedTraits>
			<li>
				<def>SpeedOffset</def>
				<degree>-1</degree>
			</li>
		</suppressedTraits>
		<biostatCpx>1</biostatCpx>
		<biostatMet>7</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>stone</symbol>
				</li>
				<li>
					<symbol>rock</symbol>
				</li>
				<li>
					<symbol>rocknroll</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/XMonsters_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/XMonsters_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
